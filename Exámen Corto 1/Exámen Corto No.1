N = int(input("Ingrese un número entero \n"))
contador = 0
factores = []

for xA in range (1, N):
    if (N % xA == 0):
        factores.append(xA)
        contador += xA
print("Los factores de", N, "son:", factores)

if (contador == N):
    print(N,"Es un número perfecto")
else:
    print(N,"No es un perfeto")






